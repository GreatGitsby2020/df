{
	"name": "UploadToSalesforce",
	"properties": {
		"activities": [
			{
				"name": "CopyAccountToSF",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": "SELECT * FROM dbo.Account WHERE ParentAccountId IS NOT NULL",
						"queryTimeout": "02:00:00"
					},
					"sink": {
						"type": "SalesforceSink",
						"writeBatchSize": 5000,
						"writeBehavior": "upsert",
						"externalIdFieldName": "BpID__c",
						"ignoreNullValues": false
					},
					"enableStaging": false,
					"enableSkipIncompatibleRow": true,
					"redirectIncompatibleRowSettings": {
						"linkedServiceName": {
							"referenceName": "AzureBlobStorageDf",
							"type": "LinkedServiceReference"
						},
						"path": "dfinput"
					},
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "Name",
									"type": "String"
								},
								"sink": {
									"name": "Name",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "ParentAccountId",
									"type": "String"
								},
								"sink": {
									"name": "Account.ParentId.BpID__c"
								}
							},
							{
								"source": {
									"name": "Street",
									"type": "String"
								},
								"sink": {
									"name": "BillingStreet",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "Phone",
									"type": "String"
								},
								"sink": {
									"name": "Phone",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "SAPId",
									"type": "String"
								},
								"sink": {
									"name": "BpID__c",
									"type": "String"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "AzureSqlAccount",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "SalesforceAccount",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}